{this.props.authenticated ? (
  <div>
    <Tab label="Dashboard" component={Link} to="/dashboard"/>
    <Tab label="Log Out" component={Link} to="/logout"/>
  </div>
    ) : (
      <div>
    <Tab label="Sign Up" component={Link} to="/signup"/>
    <Tab label="Log In" component={Link} to="/login"/>
  </div>
)}
